{
  "name": "memu-js",
  "type": "module",
  "version": "0.1.11",
  "packageManager": "pnpm@10.15.0",
  "description": "MemU JavaScript SDK for interacting with MemU API services",
  "author": "MemU Team",
  "license": "MIT",
  "homepage": "https://github.com/NevaMind-AI/MemU#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NevaMind-AI/MemU.git",
    "directory": "memu/sdk/javascript"
  },
  "bugs": {
    "url": "https://github.com/NevaMind-AI/MemU/issues"
  },
  "keywords": [
    "memu",
    "memory",
    "ai",
    "sdk",
    "api",
    "client",
    "javascript",
    "typescript"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "pkgroll",
    "build:watch": "pkgroll --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm build",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.3.0",
    "@moeru/eslint-config": "0.1.0-beta.10",
    "@moeru/tsconfig": "0.1.0-beta.10",
    "@types/node": "^24.4.0",
    "eslint": "^9.35.0",
    "pkgroll": "^2.15.4",
    "typescript": "~5.8.3"
  }
}
